{
  "name": "webviews-vite",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build:dev": "NODE_ENV=development vite build",
    "build:prod": "NODE_ENV=production vite build",
    "preview:dev": "vite preview --outDir dist_dev",
    "preview:prod": "vite preview --outDir dist_prod",
    "cleanup": "find . -type d \\( -name 'node_modules' -o -name '.vscode-test' -o -name 'dist' -o -name 'out' -o -name 'dist_dev' -o -name 'dist_prod' \\) -exec rm -rf {} + && find . -type f -name 'yarn.lock' -exec sh -c '> {}' \\; && echo 'Cleanup completed!'"
  },
  "type": "module",
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@tsconfig/svelte": "^5.0.4",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.2",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "postcss-load-config": "^6.0.1",
    "svelte": "^4.2.18",
    "svelte-preprocess": "^6.0.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5",
    "vite": "^5.3.0"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "sortablejs": "^1.15.2",
    "svelte-sortablejs": "^0.2.0"
  }
}
